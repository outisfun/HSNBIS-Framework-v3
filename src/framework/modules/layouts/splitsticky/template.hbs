
<div class = "isf-layout_split-sticky js--split-sticky split-sticky--{{ stickyPosition }}" id = "{{ modId }}">

    <div class = "isf-layout_split-sticky__sticky js--split-sticky__sticky style--something">
        <div class = "isf-layout_split-sticky__sticky__image--wrapper">
        	<picture class = "isf-el_picture js--picture">
            <source srcset="{{ stickyImage }}"
                media="(min-width: 768px)">
            <img class = "isf-layout_split-sticky__sticky__image" src = "{{ stickyImage }}" />
          </picture>
        </div>
    </div>

    <div class = "isf-layout_split-sticky__content js--split-sticky__content js--content">
        <div class = "isf-layout_split-sticky__content--inner js--split-sticky__content--inner js--container">
           <!-- content -->
        </div>
    </div>
</div>
